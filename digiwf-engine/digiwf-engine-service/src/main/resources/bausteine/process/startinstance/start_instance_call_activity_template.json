        "type": "camunda:in",
        "target": "app_topic_name",
        "expression": true
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Event Message",
      "type": "Hidden",
      "value": "startProcessV01",
      "editable": false,
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "target": "app_message_name"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Responseless",
      "type": "Hidden",
      "value": "true",
      "editable": false,
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "target": "app_responseless"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Event Type",
      "type": "Hidden",
      "value": "startProcessV01",
      "editable": false,
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "target": "app_type_name"
      },
      "constraints": {
        "notEmpty": true
      }
